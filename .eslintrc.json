{
  "extends": ["plugin:react/recommended", "@appium/eslint-config-appium"],
  "parser": "@babel/eslint-parser",
  "plugins": ["import", "react", "react-native", "@babel"],
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "@babel/arrow-parens": 0,
    "import/no-unresolved": [
      2,
      {
        "ignore": ["electron"]
      }
    ],
    "react/prop-types": 0,
    "react-native/no-inline-styles": 2
  }
}
