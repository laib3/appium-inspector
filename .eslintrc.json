{
  "extends": ["plugin:react/recommended", "@appium/eslint-config-appium-ts"],
  "plugins": ["import", "react", "react-native"],
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "react/prop-types": 0,
    "react-native/no-inline-styles": 2
  }
}
